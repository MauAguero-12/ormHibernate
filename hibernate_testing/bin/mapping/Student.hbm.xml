<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.5.6//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.5.6.dtd">  
  
 <hibernate-mapping>  
  <class name="hibernate_testing.entities.Student" table="Student">  
    <id name="id" column="id"></id>  
            
    <property name="first_name" column="first_name"></property>
    <property name="last_name" column="last_name"></property>
    <property name="email" column="email"></property>  

  </class>  
            
 </hibernate-mapping>  